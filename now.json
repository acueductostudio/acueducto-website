{
  "name": "acueducto",
  "routes": [
    {
      "src": "/(.*)/",
      "status": 301,
      "headers": { "Location": "/$1" },
      "continue": true
    },
    { "src": "/robots.txt", "dest": "/static/robots.txt" },
    { "src": "/sitemap.xml", "dest": "/static/sitemap.xml" },
    { "handle": "filesystem" }
  ]
}
